<h1><%= @employee.full_name %></h1>
<p>Alias: <%= @employee.alias %></p>
<p>Title: <%= @employee.title %></p>
<p>Office: <%= @employee.office %></p>
<%= image_tag(@employee.img_url)%>
<p><%= link_to "Edit this employee", edit_employee_path(@employee) %></p>
<p><%= link_to "Delete this employee", @employee, method: :delete %></p>